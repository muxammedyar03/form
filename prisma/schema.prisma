generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(cuid())
  username String @unique
  password String
}

model Application {
  id          String   @id @default(cuid())
  name        String
  surname     String
  phoneNumber String
  message     String
  createdAt   DateTime @default(now())
}
